<!-- URDF for M-20/25-18D (M-20iD/25) -->
<robot xmlns:xacro="http://wiki.ros.org/xacro">
  <xacro:macro
    name="m20_25-18d"
    params="prefix=''
            parent
            *origin
            child"
  >

    <xacro:property name="j1_lower_limit" value="${radians(-185)}" />
    <xacro:property name="j2_lower_limit" value="${radians(-100)}" />
    <xacro:property name="j3_lower_limit" value="${radians(-90)}" />
    <xacro:property name="j4_lower_limit" value="${radians(-200)}" />
    <xacro:property name="j5_lower_limit" value="${radians(-180)}" />
    <xacro:property name="j6_lower_limit" value="${radians(-450)}" />

    <xacro:property name="j1_upper_limit" value="${radians( 185)}" />
    <xacro:property name="j2_upper_limit" value="${radians( 160)}" />
    <xacro:property name="j3_upper_limit" value="${radians( 220)}" />
    <xacro:property name="j4_upper_limit" value="${radians( 200)}" />
    <xacro:property name="j5_upper_limit" value="${radians( 180)}" />
    <xacro:property name="j6_upper_limit" value="${radians( 450)}" />

    <xacro:property name="j1_effort_limit" value=" 7000" />
    <xacro:property name="j2_effort_limit" value=" 8000" />
    <xacro:property name="j3_effort_limit" value=" 4000" />
    <xacro:property name="j4_effort_limit" value=" 300" />
    <xacro:property name="j5_effort_limit" value=" 300" />
    <xacro:property name="j6_effort_limit" value=" 200" />

    <xacro:property name="j1_velocity_limit" value="${radians( 210)}" />
    <xacro:property name="j2_velocity_limit" value="${radians( 210)}" />
    <xacro:property name="j3_velocity_limit" value="${radians( 265)}" />
    <xacro:property name="j4_velocity_limit" value="${radians( 420)}" />
    <xacro:property name="j5_velocity_limit" value="${radians( 420)}" />
    <xacro:property name="j6_velocity_limit" value="${radians( 720)}" />

    <!-- links -->
    <link name="${prefix}base_link">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh
            filename="package://fanuc_m20_description/meshes/m20_25-18d/visual/base.dae"
          />
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh
            filename="package://fanuc_m20_description/meshes/m20_25-18d/collision/base.stl"
          />
        </geometry>
      </collision>
      <inertial>
        <mass value=" 60" />
        <origin xyz="-.003  .0008  .1" />
        <inertia
          ixx=" .8"
          ixy="-.002"
          ixz="-.0006"
          iyy=" .8"
          iyz="-.005"
          izz=" .9"
        />
      </inertial>
    </link>

    <link name="${prefix}J1_link">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh
            filename="package://fanuc_m20_description/meshes/m20_25-18d/visual/j1.dae"
          />
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh
            filename="package://fanuc_m20_description/meshes/m20_25-18d/collision/j1.stl"
          />
        </geometry>
      </collision>
      <inertial>
        <mass value=" 90" />
        <origin xyz=" .03 -.004 -.05" />
        <inertia
          ixx=" 1"
          ixy="-.3"
          ixz=" .09"
          iyy=" 1"
          iyz="-.04"
          izz=" 2"
        />
      </inertial>
    </link>

    <link name="${prefix}J2_link">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh
            filename="package://fanuc_m20_description/meshes/m20_25-18d/visual/j2.dae"
          />
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh
            filename="package://fanuc_m20_description/meshes/m20_25-18d/collision/j2.stl"
          />
        </geometry>
      </collision>
      <inertial>
        <mass value=" 50" />
        <origin xyz="-.04 -.2  .4" />
        <inertia
          ixx=" 5"
          ixy=" .04"
          ixz=" .04"
          iyy=" 6"
          iyz=" .4"
          izz=" .4"
        />
      </inertial>
    </link>

    <link name="${prefix}J3_link">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh
            filename="package://fanuc_m20_description/meshes/m20_25-18d/visual/j3.dae"
          />
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh
            filename="package://fanuc_m20_description/meshes/m20_25-18d/collision/j3.stl"
          />
        </geometry>
      </collision>
      <inertial>
        <mass value=" 40" />
        <origin xyz=" .1  .02  .1" />
        <inertia
          ixx=" .6"
          ixy=" .01"
          ixz=" .3"
          iyy=" .8"
          iyz=" .005"
          izz=" .6"
        />
      </inertial>
    </link>

    <link name="${prefix}J4_link">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh
            filename="package://fanuc_m20_description/meshes/m20_25-18d/visual/j4.dae"
          />
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh
            filename="package://fanuc_m20_description/meshes/m20_25-18d/collision/j4.stl"
          />
        </geometry>
      </collision>
      <inertial>
        <mass value=" 20" />
        <origin xyz=" .6 -.06 -.002" />
        <inertia
          ixx=" .06"
          ixy="-.07"
          ixz=" .008"
          iyy=" .6"
          iyz=" .00002"
          izz=" .6"
        />
      </inertial>
    </link>

    <link name="${prefix}J5_link">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh
            filename="package://fanuc_m20_description/meshes/m20_25-18d/visual/j5.dae"
          />
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh
            filename="package://fanuc_m20_description/meshes/m20_25-18d/collision/j5.stl"
          />
        </geometry>
      </collision>
      <inertial>
        <mass value=" 4" />
        <origin xyz=" .02 -.04 -.0001" />
        <inertia
          ixx=" .01"
          ixy=" .003"
          ixz="-.00001"
          iyy=" .01"
          iyz="-.00004"
          izz=" .01"
        />
      </inertial>
    </link>

    <link name="${prefix}J6_link">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh
            filename="package://fanuc_m20_description/meshes/m20_25-18d/visual/j6.dae"
          />
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh
            filename="package://fanuc_m20_description/meshes/m20_25-18d/collision/j6.stl"
          />
        </geometry>
      </collision>
      <inertial>
        <mass value=" 1" />
        <origin xyz=" .06  0  0" />
        <inertia
          ixx=" .001"
          ixy=" 0"
          ixz=" 0"
          iyy=" .0008"
          iyz=" .0000004"
          izz=" .0008"
        />
      </inertial>
    </link>

    <!-- joints -->
    <joint name="${prefix}J1" type="revolute">
      <origin xyz=" 0  0  .425" rpy="0 0 0" />
      <parent link="${prefix}base_link" />
      <child link="${prefix}J1_link" />
      <axis xyz=" 0  0  1" />
      <limit
        lower="${j1_lower_limit}"
        upper="${j1_upper_limit}"
        effort="${j1_effort_limit}"
        velocity="${j1_velocity_limit}"
      />
    </joint>

    <joint name="${prefix}J2" type="revolute">
      <origin xyz=" .075  0  0" rpy="0 0 0" />
      <parent link="${prefix}J1_link" />
      <child link="${prefix}J2_link" />
      <axis xyz=" 0  1  0" />
      <limit
        lower="${j2_lower_limit}"
        upper="${j2_upper_limit}"
        effort="${j2_effort_limit}"
        velocity="${j2_velocity_limit}"
      />
    </joint>

    <joint name="${prefix}J3" type="revolute">
      <origin xyz=" 0  0  .84" rpy="0 0 0" />
      <parent link="${prefix}J2_link" />
      <child link="${prefix}J3_link" />
      <axis xyz=" 0 -1  0" />
      <limit
        lower="${j3_lower_limit}"
        upper="${j3_upper_limit}"
        effort="${j3_effort_limit}"
        velocity="${j3_velocity_limit}"
      />
    </joint>

    <joint name="${prefix}J4" type="revolute">
      <origin xyz=" 0  0  .215" rpy="0 0 0" />
      <parent link="${prefix}J3_link" />
      <child link="${prefix}J4_link" />
      <axis xyz="-1  0  0" />
      <limit
        lower="${j4_lower_limit}"
        upper="${j4_upper_limit}"
        effort="${j4_effort_limit}"
        velocity="${j4_velocity_limit}"
      />
    </joint>

    <joint name="${prefix}J5" type="revolute">
      <origin xyz=" .89  0  0" rpy="0 0 0" />
      <parent link="${prefix}J4_link" />
      <child link="${prefix}J5_link" />
      <axis xyz=" 0 -1  0" />
      <limit
        lower="${j5_lower_limit}"
        upper="${j5_upper_limit}"
        effort="${j5_effort_limit}"
        velocity="${j5_velocity_limit}"
      />
    </joint>

    <joint name="${prefix}J6" type="revolute">
      <origin xyz=" 0  0  0" rpy="0 0 0" />
      <parent link="${prefix}J5_link" />
      <child link="${prefix}J6_link" />
      <axis xyz="-1  0  0" />
      <limit
        lower="${j6_lower_limit}"
        upper="${j6_upper_limit}"
        effort="${j6_effort_limit}"
        velocity="${j6_velocity_limit}"
      />
    </joint>

    <!-- world coords -->
    <link name="${prefix}wbase" />
    <joint name="${prefix}base_link-wbase" type="fixed">
      <origin xyz=" 0  0  .425" rpy="0 0 0" />
      <parent link="${prefix}base_link" />
      <child link="${prefix}wbase" />
    </joint>

    <!-- flange coords -->
    <link name="${prefix}flange" />
    <joint name="${prefix}J6-flange" type="fixed">
      <origin xyz=" .09  0  0" rpy="0 0 0" />
      <parent link="${prefix}J6_link" />
      <child link="${prefix}flange" />
    </joint>

    <!-- Attach to parent -->
    <joint name="${prefix}base_joint" type="fixed">
      <xacro:insert_block name="origin" />
      <parent link="${parent}" />
      <child link="${prefix}base_link" />
    </joint>

    <!-- Attach child -->
    <joint name="${prefix}child_joint" type="fixed">
      <origin xyz="0 0 0" rpy="0 0 0" />
      <parent link="${prefix}flange" />
      <child link="${child}" />
    </joint>
  </xacro:macro>
</robot>
